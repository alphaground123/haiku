#!/usr/bin/env node

var haiku = require('../lib/haiku')
;

process.title = 'haiku'

haiku.cli.on('--help', function(){
  console.log('here is some help');
});

haiku.cli.on('--version', function(){
  console.log('version');
});

process.on("uncaughtException", function(err){
  console.error("Uncaught exception: " + err.stack);
  process.exit(1);
});

haiku.cli.start();
